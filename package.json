{
  "name": "@deepcase/deeplinks-app",
  "version": "0.0.16",
  "scripts": {
    "migrate": "export MIGRATIONS_HASURA_PATH=localhost:8080; export MIGRATIONS_HASURA_SSL=0; export MIGRATIONS_HASURA_SECRET=myadminsecretkey; (cd node_modules/@deepcase/deeplinks; npm run unmigrate; npm run migrate;)",
    "heroku-start": "node start.js",
    "heroku-postbuild": "echo $HEROKU_APP_NAME; NEXT_PUBLIC_HASURA_SSL=1 NEXT_PUBLIC_HASURA_PATH=deep-app-menz.herokuapp.com/hasura PORT=$NEXTPORT && next build",
    "heroku-next-start": "next start -p $PORT",
    "unsafe-dev": "next dev -p $PORT",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "dev": "npm run unsafe-dev",
    "package:publish": "npm run build; npm publish --access public;"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@deepcase/deeplinks": "^0.0.17",
    "@graphiql/toolkit": "^0.2.2",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@react-hook/debounce": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-load": "^2.0.0",
    "graphiql": "^1.4.2",
    "http-proxy-middleware": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^10.2.3",
    "next-env": "^1.1.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-force-graph": "^1.41.3",
    "react-ga": "^3.3.0",
    "react-monaco-editor": "^0.43.0",
    "react-resize-detector": "^6.7.1",
    "react-yandex-metrika": "^2.6.0",
    "three-spritetext": "^1.6.2",
    "use-debounce": "^6.0.1"
  },
  "engines": {
    "node": ">=15.10",
    "npm": ">=7.5.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "dotenv-webpack": "^7.0.2",
    "webpack": "^5.28.0"
  }
}
